<%= simple_form_for @list do |form| %>
  <div class="form-group">
    <%= form.text_field :name, placeholder: "name" %>
  <div>
  <div class="form-group">
    <%= form.text_field :description, placeholder: "description" %>
  </div>
  <div class="form-group">
    <%= form.hidden_field :status, value: true%>
  </div>
  <%= form.submit "Submit", class: "btn btn-default btn-primary" %>
<% end %>
